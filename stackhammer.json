{
  "metaVersion": "1.1.3",
  "puppetVersion": "PUPPET27",
  "environments": [
    {
      "siteLocation": "manifests",
      "moduleLocations": [
        "modules"
      ],
      "name": "production"
    }
  ],
  "tests": [
    {
      "rakeFile": "rakewrappers/stdlib.rake",
      "rakeTask": "default",
      "outputs": [
        {
          "location": "modules/stdlib/spec/reports",
          "name": "all_tests"
        }
      ],
      "name": "stdlib_module"
    }
  ]
}